{
    "env": {
        "amd": true,
        "browser": true,
        "node": true
    },
    "globals": {
        "moment": false
    },
    "rules": {
        "camelcase": ["off"],
        "comma-dangle": ["off"],
        "comma-spacing": ["off"],
        "consistent-return": ["off"],
        "curly": ["off"],
        "default-case": ["off"],
        "eqeqeq": ["off"],
        "func-names": ["off"],
        "guard-for-in": ["off"],
        "indent": ["off"],
        "key-spacing": ["off"],
        "keyword-spacing": ["off"],
        "max-len": ["off"],
        "new-cap": ["off"],
        "no-cond-assign": ["off"],
        "no-func-assign": ["off"],
        "no-nested-ternary": ["off"],
        "no-param-reassign": ["off"],
        "no-sequences": ["off"],
        "no-shadow": ["off"],
        "no-shadow-restricted-names": ["off"],
        "no-unused-expressions": ["off"],
        "no-unused-vars": ["off"],
        "no-use-before-define": ["off"],
        "no-var": ["off"],
        "one-var": ["off"],
        "one-var-declaration-per-line": ["off"],
        "prefer-arrow-callback": ["off"],
        "prefer-rest-params": ["off"],
        "prefer-template": ["off"],
        "quotes": ["off"],
        "semi": ["off"],
        "semi-spacing": ["off"],
        "space-before-blocks": ["off"],
        "space-before-function-paren": ["off"],
        "space-infix-ops": ["off"],
        "spaced-comment": ["off"],
        "vars-on-top": ["off"],
        "wrap-iife": ["off"],
        "yoda": ["off"]
    }
}
